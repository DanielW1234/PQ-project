#include <iostream>
using namespace std;

int main() {
    int a = 100;
    int b = 200;
    int c = a + b;
    cout << "c = " << c << endl;  

    // 添加乘法功能
    int e = a * b;
    cout << "e = " << e << endl;

    // 添加减法功能
    int d = b - a;
    cout << "d = " << d << endl;
    
    // 添加除法功能
    int f = b / a;
    cout << "f = " << f << endl;

    return 0;
}
